snippet     [p
alias       [package
abbr        [package] ....
options     word, indent
    [package]
    name = "${0}"
    version = ""
    authors = [""]
    description = ""
    license = "MIT OR Apache-2.0"
snippet      [b
alias       build-op
abbr        [profile.dev] .... [profile.relese] .....
options     word, indent
    # 最適化オプション
    # 0-3の範囲で指定
    [profile.dev]
    opt-level = 0
    [profile.release]
    opt-level = 3
    panic = 'abort'${0}
snippet     [l
alias       linker
abbr        [target.aarch64] linker = "/bin/clang" rustflags = ["-Clink-arg=-fuse-ld=/usr/local/bin/mold"]
options     word, indent
    [target.aarch64]
    linker = "/bin/clang"
    rustflags = ["-Clink-arg=-fuse-ld=/usr/local/bin/mold"]${0}
snippet     [dev
abbr        [dev-dependencies] assert_cmd = "2" predicates = "2"
options     word, indent
    [dev-dependencies]
    assert_cmd = "2"
    predicates = "2"

